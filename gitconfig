[color]
	ui = auto
[alias]
	ap = add -p
	au = add -u
	br = branch
	ci = commit
	cia = commit --amend
	ciaa = commit --amend -C HEAD
	cip = commit --patch
	co = checkout
	cp = cherry-pick
	dc = diff --cached
	df = diff
	fe = fetch
	l = log --graph --decorate --all
	ol = log --pretty=oneline
	pt = push -u origin
	r = rebase
	ra = rebase --abort
	rc = rebase --continue
	ri = rebase --interactive --autosquash
	ro = rebase --onto
	rs = rebase --skip
	s = stash
	sha = "!f() { git rev-parse --default HEAD $* |tee /dev/tty |tr -d '\n' |pbcopy; }; f"
	sk = stash --keep-index
	sp = stash pop
	st = status -sb
	su = submodule update --init
	x = !gitx
	ss = show stash
	ma = merge --abort
	pushu = push --set-upstream origin HEAD
	cpa = cherry-pick --abort
[core]
	excludesfile = ~/.gitignore
	editor = mvim -f
[push]
	default = upstream
[merge]
	ff = false
[branch]
	autosetuprebase = always
	autosetupmerge = true
[advice]
	pushNonFastForward = false
	statusHints = false
	commitBeforeMerge = false
	resolveConflict = false
	detachedHead = false
[rerere]
	enabled = true
[diff]
	renames = true
[rebase]
	autosquash = true
[help]
	autocorrect = 1
[interactive]
	singlekey = true
